
{
  "name": "grievance-portal-database-migration",
  "version": "1.0.0",
  "description": "Production-ready database migration tool for Grievance Portal",
  "main": "migrate_from_supabase.js",
  "scripts": {
    "migrate": "node migrate_from_supabase.js",
    "test-connections": "node -e \"require('./migrate_from_supabase').testConnections().then(success => process.exit(success ? 0 : 1))\"",
    "verify": "node -e \"require('./migrate_from_supabase').verifyMigration().then(success => process.exit(success ? 0 : 1))\"",
    "migrate:dry-run": "LOG_LEVEL=debug node migrate_from_supabase.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "mysql2": "^3.6.0",
    "dotenv": "^16.3.1"
  },
  "keywords": [
    "database",
    "migration",
    "mysql",
    "supabase",
    "production"
  ],
  "author": "Grievance Portal Team",
  "license": "MIT"
}
